package com.MedImager.ExaminationServer;

import java.util.ArrayList;
import java.util.List;

public class Collection {
	private String collectionName;
	private String collectionID;
	private List<CollectionItem> collection;
	public Collection(){
		this.collection = new ArrayList<CollectionItem>();
	}
	public List<CollectionItem> getCollection() {
		return collection;
	}
	public List<Examination> getExaminations(){
		ExaminationBuilder examinationBuilder = new ExaminationBuilder();
		for(CollectionItem item:collection){
			examinationBuilder.getExamination(item.getExaminationID())
		}
	}
	
	public void setCollection(List<CollectionItem> collection) {
		this.collection = collection;
	}
	public void addItem(CollectionItem item){
		collection.add(item);
	}
	public String getCollectionName() {
		return collectionName;
	}
	public void setCollectionName(String collectionName) {
		this.collectionName = collectionName;
	}
	public String getCollectionID() {
		return collectionID;
	}
	public void setCollectionID(String collectionID) {
		this.collectionID = collectionID;
	}
}
