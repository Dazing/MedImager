import java.util.List;
import java.util.Map;

public class InitValues {
	public static final String[] relevantTerms = {
			"Age", "Allergy", "Biopsy-site", "Diag-def", "Diag-hist", 
			"Diag-Tent", "Dis-now", "Dis-past", "Drug", "Factor-neg", 
