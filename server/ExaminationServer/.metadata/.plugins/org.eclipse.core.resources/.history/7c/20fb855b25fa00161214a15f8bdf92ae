<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script>
function resultJSON(){
	$("#imgContainer").empty();
	console.log($("#searchTerm").val());
	$.getJSON("examData/search/" + $("#searchTerm").val(), function(data){
		data.forEach(function(exam){
			exam.imagePaths.forEach(function(path){
				$("#imgContainer").prepend($('<img>',{id:'theImg',src:path}))
				console.log(path);
			});
		});
	});
}

</script>
</head>
<input type=text id="searchTerm">
<input type="submit" onclick="resultJSON();">
<div id="imgContainer">
</div>
</body>
</html>