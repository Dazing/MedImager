     *
     * @return String that will be returned as a text/plain response.
     */
	class Fruit{
		public Fruit(){
		}
		public String getFruitType() {
			return fruitType;
		}
		public void setFruitType(String fruitType) {
			this.fruitType = fruitType;
		}
		private String fruitType;
		
		@Override
		public String toString(){
			return fruitType;
		}
		
	}
